{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.9/MicrosoftTeams.schema.json",
    "manifestVersion": "1.9",
    "version": "1.0.0",
    "id": "<<YOUR-MICROSOFT-APP-ID>>",
    "packageName": "com.microsoft.teams.adaptive",
    "developer": {
      "name": "Microsoft",
      "websiteUrl": "https://example.azurewebsites.net",
      "privacyUrl": "https://example.azurewebsites.net/privacy",
      "termsOfUseUrl": "https://example.azurewebsites.net/termsofuse"
    },
    "icons": {
      "color": "color.png",
      "outline": "outline.png"
    },
    "name": {
      "short": "Sequential Workflows",
      "full": "Sequential Workflows"
    },
    "description": {
      "short": "Sequential Workflows in adaptive card Scenario",
      "full": "Sequential Workflows in adaptive card Scenario"
    },
    "accentColor": "#FFFFFF",
    "bots": [
      {
        "botId": "<<YOUR-MICROSOFT-APP-ID>>",
        "scopes": [
            "personal",
            "team",
            "groupchat"
        ],
        "commandLists": [
          {
              "scopes": [
                  "groupchat"
              ],
              "commands": [
                  {
                      "title": "Incident Management",
                      "description": "Starts the Incident Management Workflow"
                  }
              ]
          }
      ],
      "isNotificationOnly": false,
      "needsChannelSelector": false
      }
    ],
    "permissions": [
      "identity",
      "messageTeamMembers"
    ],
    "validDomains": [
      "*.ngrok.io"
    ]
  }