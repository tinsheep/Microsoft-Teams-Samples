{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.12/MicrosoftTeams.schema.json",
  "manifestVersion": "1.12",
  "version": "1.0",
  "id": "<<Your-Microsoft-App-Id>>",
  "packageName": "com.microsoft.teams.sample.deeplinkBot",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "",
    "privacyUrl": "",
    "termsOfUseUrl": ""
  },
  "name": {
    "short": "DeepLink Bot"
  },
  "description": {
    "short": "DeepLink Bot",
    "full": "DeepLink Bot for channel and static tabs"
  },
  "icons": {
    "outline": "icon-outline.png",
    "color": "icon-color.png"
  },
  "accentColor": "#ffffff",
  "configurableTabs": [
    {
      "configurationUrl": "https://<<BASE-URL>>/Configure.html",
      "canUpdateConfiguration": true,
      "scopes": [
        "groupchat",
        "team"
      ]
    }
  ],

  "staticTabs": [
    {
      "contentUrl": "https://<<BASE-URL>>/DeepLink.html",
      "websiteUrl": "https://<<BASE-URL>>",
      "entityId": "com.contoso.DeeplLinkBot.help",
      "name": "Deep Link Tab",
      "scopes": [
        "personal"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "*.ngrok.io"
  ],
  "bots": [
    {
      "botId": "<<Your-Microsoft-App-Id>>",
      "scopes": [
        "groupchat",
        "personal",
        "team"
      ],

      "isNotificationOnly": false,
      "supportsFiles": false
    }
  ]
}